bind-key j next
bind-key k prev

unbind-key g
bind-key g home
bind-key G end

auto-reload yes
reload-threads 100

browser w3m
macro m set browser "/usr/bin/mpv --no-terminal %u"; open-in-browser ; set browser "/usr/bin/w3m %u"
macro f set browser "/usr/bin/firefox %u"; open-in-browser ; set browser "/usr/bin/w3m %u"
